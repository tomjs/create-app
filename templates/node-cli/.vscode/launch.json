  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug CLI",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "tsx",
      "program": "${workspaceFolder}\\src\\index.ts",
      "args": [
        "--config",
        "${workspaceFolder}\\xxx.config.cjs"
      ],
      "console": "integratedTerminal"
    }
  ]
}
