{
  "publisher": "",
  "name": "vscode-vue",
  "displayName": "%displayName%",
  "type": "commonjs",
  "description": "%description%",
  "license": "MIT",
  "keywords": [
    "vscode"
  ],
  "categories": [
    "Other"
  ],
  "main": "dist/extension/index.js",
  "icon": "resources/logo.png",
  "engines": {
    "node": ">=14.16.0",
    "vscode": "^1.56.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "tomjs.xxx.showHello",
        "title": "%tomjs.commands.hello%",
        "category": "%displayName%"
      },
      {
        "command": "tomjs.xxx.showPanel",
        "title": "%tomjs.commands.panel%",
        "category": "%displayName%"
      }
    ]
  },
  "vsce": {
    "dependencies": false,
    "yarn": false
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vsd": "vscode-dev --watch",
    "dev:dist": "vite",
    "build": "vscode-dev && tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tomjs/node": "^2.2.3",
    "@tomjs/vscode": "^2.5.0",
    "@tomjs/vscode-webview": "^2.0.2",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tomjs/vite-plugin-vscode": "^5.1.0",
    "@tomjs/vscode-dev": "^3.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.56.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "vite": "^7.2.4"
  }
}
