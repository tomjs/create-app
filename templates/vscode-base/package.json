    "dev:dist": "tsdown --watch",
    "build": "vscode-dev && tsdown --minify"
  },
  "dependencies": {
    "@tomjs/node": "^2.2.3",
    "@tomjs/vscode": "^2.5.0"
  },
  "devDependencies": {
    "@tomjs/vscode-dev": "^3.1.2",
    "@types/vscode": "^1.56.0",
    "tsdown": "^0.19.0"
  }
