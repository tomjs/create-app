{
  "publisher": "",
  "name": "vscode-base",
  "displayName": "%displayName%",
  "type": "commonjs",
  "description": "%description%",
  "license": "MIT",
  "keywords": [
    "vscode"
  ],
  "categories": [
    "Other"
  ],
  "main": "dist/index.js",
  "icon": "resources/logo.png",
  "engines": {
    "node": ">=14.16.0",
    "vscode": "^1.56.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "tomjs.xxx.showHello",
